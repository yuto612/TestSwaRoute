{
  "routes": [
    {
      "route": "/android",
      "serve": "/android/index.html"
    },
    {
      "route": "/android/Build/{*rest}",
      "serve": "/android/Build/{*rest}"
    },
    {
      "route": "/android/TemplateData/{*rest}",
      "serve": "/android/TemplateData/{*rest}"
    },
    {
      "route": "/ios",
      "serve": "/ios/index.html"
    },
    {
      "route": "/ios/Build/{*rest}",
      "serve": "/ios/Build/{*rest}"
    },
    {
      "route": "/ios/TemplateData/{*rest}",
      "serve": "/ios/TemplateData/{*rest}"
    }
  ],
  "mimeTypes": {
    ".wasm": "application/wasm",
    ".data": "application/octet-stream",
    ".js": "application/javascript",
    ".css": "text/css"
  }
}
