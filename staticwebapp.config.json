{
  "routes": [
    {
      "route": "/android",
      "serve": "/android/index.html"
    },
    {
      "route": "/android/Build/*.js",
      "serve": "/android/Build/*.js"
    },
    {
      "route": "/android/Build/*.wasm",
      "serve": "/android/Build/*.wasm"
    },
    {
      "route": "/android/Build/*.data",
      "serve": "/android/Build/*.data"
    },
    {
      "route": "/android/TemplateData/*.css",
      "serve": "/android/TemplateData/*.css"
    },
    {
      "route": "/ios",
      "serve": "/ios/index.html"
    },
    {
      "route": "/ios/Build/*.js",
      "serve": "/ios/Build/*.js"
    },
    {
      "route": "/ios/Build/*.wasm",
      "serve": "/ios/Build/*.wasm"
    },
    {
      "route": "/ios/Build/*.data",
      "serve": "/ios/Build/*.data"
    },
    {
      "route": "/ios/TemplateData/*.css",
      "serve": "/ios/TemplateData/*.css"
    }
  ],
  "mimeTypes": {
    ".wasm": "application/wasm",
    ".data": "application/octet-stream",
    ".js": "application/javascript",
    ".css": "text/css"
  }
}
