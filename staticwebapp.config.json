{
  "routes": [
    {
      "route": "/android",
      "serve": "/android/index.html"
    },
    {
      "route": "/android/Build/{file}",
      "serve": "/android/Build/{file}"
    },
    {
      "route": "/android/TemplateData/{file}",
      "serve": "/android/TemplateData/{file}"
    },
    
    {
      "route": "/ios",
      "serve": "/ios/index.html"
    },
    {
      "route": "/ios/Build/{file}",
      "serve": "/ios/Build/{file}"
    },
    {
      "route": "/ios/TemplateData/{file}",
      "serve": "/ios/TemplateData/{file}"
    }
  ],
  "mimeTypes": {
    ".wasm": "application/wasm",
    ".data": "application/octet-stream",
    ".js": "application/javascript",
    ".css": "text/css",
    ".ico": "image/x-icon"
  }
}
